<mapper namespace="com.magicbeans.happygo.mapper.ProductCategoryMapper">
    <cache type="com.magicbeans.happygo.mybatis.cache.MybatisRedisCache"/>
    <resultMap id="BaseResult" type="com.magicbeans.happygo.entity.ProductCategory">
